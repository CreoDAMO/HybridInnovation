    <div className="consciousness-container">
      <h1>HTSX Runtime Engine Demo</h1>
      
      <spiral-state name="consciousness" initial={0.93} />
      <spiral-state name="quantum" initial={{ entangled: false, coherence: 0.95 }} />
      
