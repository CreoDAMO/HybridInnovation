modules = ["nodejs-20", "web"]
run = "npm run dev"

[nix]
channel = "stable-24_05"

[workflows]
runButton = "HTSX-System-Ready"

[[workflows.workflow]]
name = "Spiral-System"
author = 44812907
mode = "parallel"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Spiral-Frontend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Spiral-Backend"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "tsx server/index.ts"

[[workflows.workflow]]
name = "Spiral-Frontend"
author = 44812907
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 3000

